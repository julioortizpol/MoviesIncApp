  },
  "jest": {
    "setupFiles": [
      "./jestSetupFile.ts"
    ],
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?@?react-native|@react-native-community|@react-navigation)"
